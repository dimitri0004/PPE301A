{% extends 'base.html.twig' %}

{% block title %}EmploiTemps{% endblock %}

{% block body %}
    <h1>EmploiTemps</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emploi_temp.id }}</td>
            </tr>
            <tr>
                <th>Jour</th>
                <td>{{ emploi_temp.jour }}</td>
            </tr>
            <tr>
                <th>Heure_debut</th>
                <td>{{ emploi_temp.heureDebut ? emploi_temp.heureDebut|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Heure_fin</th>
                <td>{{ emploi_temp.heureFin ? emploi_temp.heureFin|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_emploi_temps_index') }}">back to list</a>

    <a href="{{ path('app_emploi_temps_edit', {'id': emploi_temp.id}) }}">edit</a>

    {{ include('emploi_temps/_delete_form.html.twig') }}
{% endblock %}
